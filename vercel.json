{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "installCommand": "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=0 npm install && npx playwright install chromium",
  "buildCommand": "npx playwright install chromium"
}